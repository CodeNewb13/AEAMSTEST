      "readings": [
        {
          "rod_id": "asdasdsa",
          "secret": "AEAMS_SECRET_zhmaj9w00ag",
          "timestamp": "2025-09-06T14:30:00Z",
          "temperature": 23.5,
          "moisture": 45.2,
          "ph": 6.8,
          "conductivity": 1.2,
          "nitrogen": 12.5,
          "phosphorus": 8.3,
          "potassium": 15.7
        }
      ]
    }
  },

