  "_usage_examples": {
    "curl_command": "curl -X POST \"https://aeams-test-production.up.railway.app/api/rod/justintul\" -H \"Content-Type: application/json\" -d @example_payload.json",
    "powershell_command": "Invoke-WebRequest -Uri \"https://aeams-test-production.up.railway.app/api/rod/justintul\" -Method POST -Headers @{\"Content-Type\"=\"application/json\"} -Body (Get-Content example_payload.json -Raw)",
    "batch_file": "Use AEAMS-All-In-One-Push.bat which automatically formats this JSON",
    "linux_script": "Use aeams-push.sh which automatically formats this JSON"
  },

  "_field_descriptions": {
    "secret": "Global authentication key - must match server configuration",
    "rod_id": "Unique identifier for the secondary sensor rod",
    "timestamp": "ISO 8601 format timestamp (UTC) - when the reading was taken",
    "temperature": "Temperature in Celsius (Â°C) - optional",
    "moisture": "Soil moisture percentage (0-100%) - optional", 
    "ph": "Soil pH level (0-14) - optional",
    "conductivity": "Electrical conductivity in mS/cm - optional",
    "nitrogen": "Nitrogen content in ppm or mg/kg - optional",
    "phosphorus": "Phosphorus content in ppm or mg/kg - optional", 
    "potassium": "Potassium content in ppm or mg/kg - optional"
  },

